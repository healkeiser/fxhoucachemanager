{
    "load_package_once": true,
    "enable": true,
    "env": [
        {
            "FXHOUCACHEMANAGER": [
                {
                    "houdini_os == 'windows'": "D:/Code/fxhoucachemanager"
                },
                {
                    "houdini_os == 'linux'": "$HOME/Code/fxhoucachemanager"
                },
                {
                    "houdini_os == 'macos'": "$HOME/Code/fxhoucachemanager"
                }
            ]
        },
        {
            "PYTHONPATH": [
                "$FXHOUCACHEMANAGER",
                {
                    "method": "append"
                }
            ]
        }
    ],
    "path": [
        "$FXHOUCACHEMANAGER/houdini",
        {
            "method": "append"
        }
    ]
}